n","level","type","min","max","value","onChange","e","target","placeholder","rows","onClick"],"sources":["/Users/tumowuh/Desktop/backups/sumki-mini-app_Release_Vers_20250401_221838/src/components/OptionsStep.tsx"],"sourcesContent":["import React, { useState } from 'react';\nimport { Typography, Row, Col, Checkbox, InputNumber, Card } from 'antd';\nimport './OptionsStep.css';\n\nconst { Title, Text } = Typography;\n\ninterface OptionsStepProps {\n  quantity: number;\n  additionalNotes: string;\n  onQuantityChange: (quantity: number) => void;\n  onNotesChange: (notes: string) => void;\n  onNext: () => void;\n  onBack: () => void;\n}\n\nconst OptionsStep: React.FC<OptionsStepProps> = ({\n  quantity,\n  additionalNotes,\n  onQuantityChange,\n  onNotesChange,\n  onNext,\n  onBack\n}) => {\n  const [options, setOptions] = useState({\n    giftWrap: false,\n    expressDelivery: false,\n    personalization: false\n  });\n\n  const handleOptionChange = (option: string, checked: boolean) => {\n    setOptions(prev => ({ ...prev, [option]: checked }));\n  };\n\n  return (\n    <div className=\"options-step\">\n      <div className=\"step-header\">\n        <Title level={3}>Дополнительные опции</Title>\n        <Text type=\"secondary\">Настройте дополнительные параметры заказа</Text>\n      </div>\n\n      <div className=\"options-content\">\n        <Card className=\"option-card\">\n          <div className=\"option-item\">\n            <div className=\"option-info\">\n              <h4>Количество</h4>\n              <Text type=\"secondary\">Выберите количество товаров</Text>\n            </div>\n            <InputNumber\n              min={1}\n              max={10}\n              value={quantity}\n              onChange={(value) => onQuantityChange(value || 1)}\n              className=\"quantity-input\"\n            />\n          </div>\n        </Card>\n\n        <Card className=\"option-card\">\n          <div className=\"option-item\">\n            <div className=\"option-info\">\n              <h4>Подарочная упаковка</h4>\n              <Text type=\"secondary\">Красивая упаковка для подарка (+200₽)</Text>\n            </div>\n            <Checkbox\n              checked={options.giftWrap}\n              onChange={(e) => handleOptionChange('giftWrap', e.target.checked)}\n            />\n          </div>\n        </Card>\n\n        <Card className=\"option-card\">\n          <div className=\"option-item\">\n            <div className=\"option-info\">\n              <h4>Экспресс доставка</h4>\n              <Text type=\"secondary\">Быстрая доставка в течение 24 часов (+500₽)</Text>\n            </div>\n            <Checkbox\n              checked={options.expressDelivery}\n              onChange={(e) => handleOptionChange('expressDelivery', e.target.checked)}\n            />\n          </div>\n        </Card>\n\n        <Card className=\"option-card\">\n          <div className=\"option-item\">\n            <div className=\"option-info\">\n              <h4>Персонализация</h4>\n              <Text type=\"secondary\">Добавить имя или текст (+300₽)</Text>\n            </div>\n            <Checkbox\n              checked={options.personalization}\n              onChange={(e) => handleOptionChange('personalization', e.target.checked)}\n            />\n          </div>\n        </Card>\n\n        <Card className=\"option-card\">\n          <div className=\"option-item\">\n            <div className=\"option-info\">\n              <h4>Дополнительные пожелания</h4>\n              <Text type=\"secondary\">Укажите особые требования к заказу</Text>\n            </div>\n            <textarea\n              value={additionalNotes}\n              onChange={(e) => onNotesChange(e.target.value)}\n              placeholder=\"Например: особые требования к дизайну, цвету или размеру...\"\n              className=\"notes-textarea\"\n              rows={3}\n            />\n          </div>\n        </Card>\n      </div>\n\n      <div className=\"step-actions\">\n        <button className=\"back-button\" onClick={onBack}>\n          Назад\n        </button>\n        <button className=\"next-button\" onClick={onNext}>\n          Продолжить\n        </button>\n      </div>\n    </div>\n  );\n};\n\nexport default OptionsStep; "],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAIC,QAAQ,KAAQ,OAAO,CACvC,OAASC,UAAU,CAAYC,QAAQ,CAAEC,WAAW,CAAEC,IAAI,KAAQ,MAAM,CACxE,MAAO,mBAAmB,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAE3B,KAAM,CAAEC,KAAK,CAAEC,IAAK,CAAC,CAAGT,UAAU,CAWlC,KAAM,CAAAU,WAAuC,CAAGC,IAAA,EAO1C,IAP2C,CAC/CC,QAAQ,CACRC,eAAe,CACfC,gBAAgB,CAChBC,aAAa,CACbC,MAAM,CACNC,MACF,CAAC,CAAAN,IAAA,CACC,KAAM,CAACO,OAAO,CAAEC,UAAU,CAAC,CAAGpB,QAAQ,CAAC,CACrCqB,QAAQ,CAAE,KAAK,CACfC,eAAe,CAAE,KAAK,CACtBC,eAAe,CAAE,KACnB,CAAC,CAAC,CAEF,KAAM,CAAAC,kBAAkB,CAAGA,CAACC,MAAc,CAAEC,OAAgB,GAAK,CAC/DN,UAAU,CAACO,IAAI,GAAK,CAAE,GAAGA,IAAI,CAAE,CAACF,MAAM,EAAGC,OAAQ,CAAC,CAAC,CAAC,CACtD,CAAC,CAED,mBACElB,KAAA,QAAKoB,SAAS,CAAC,cAAc,CAAAC,QAAA,eAC3BrB,KAAA,QAAKoB,SAAS,CAAC,aAAa,CAAAC,QAAA,eAC1BvB,IAAA,CAACG,KAAK,EAACqB,KAAK,CAAE,CAAE,CAAAD,QAAA,CAAC,qHAAoB,CAAO,CAAC,cAC7CvB,IAAA,CAACI,IAAI,EAACqB,IAAI,CAAC,WAAW,CAAAF,QAAA,CAAC,yOAAyC,CAAM,CAAC,EACpE,CAAC,cAENrB,KAAA,QAAKoB,SAAS,CAAC,iBAAiB,CAAAC,QAAA,eAC9BvB,IAAA,CAACF,IAAI,EAACwB,SAAS,CAAC,aAAa,CAAAC,QAAA,cAC3BrB,KAAA,QAAKoB,SAAS,CAAC,aAAa,CAAAC,QAAA,eAC1BrB,KAAA,QAAKoB,SAAS,CAAC,aAAa,CAAAC,QAAA,eAC1BvB,IAAA,OAAAuB,QAAA,CAAI,8DAAU,CAAI,CAAC,cACnBvB,IAAA,CAACI,IAAI,EAACqB,IAAI,CAAC,WAAW,CAAAF,QAAA,CAAC,0JAA2B,CAAM,CAAC,EACtD,CAAC,cACNvB,IAAA,CAACH,WAAW,EACV6B,GAAG,CAAE,CAAE,CACPC,GAAG,CAAE,EAAG,CACRC,KAAK,CAAErB,QAAS,CAChBsB,QAAQ,CAAGD,KAAK,EAAKnB,gBAAgB,CAACmB,KAAK,EAAI,CAAC,CAAE,CAClDN,SAAS,CAAC,gBAAgB,CAC3B,CAAC,EACC,CAAC,CACF,CAAC,cAEPtB,IAAA,CAACF,IAAI,EAACwB,SAAS,CAAC,aAAa,CAAAC,QAAA,cAC3BrB,KAAA,QAAKoB,SAAS,CAAC,aAAa,CAAAC,QAAA,eAC1BrB,KAAA,QAAKoB,SAAS,CAAC,aAAa,CAAAC,QAAA,eAC1BvB,IAAA,OAAAuB,QAAA,CAAI,+GAAmB,CAAI,CAAC,cAC5BvB,IAAA,CAACI,IAAI,EAACqB,IAAI,CAAC,WAAW,CAAAF,QAAA,CAAC,8KAAqC,CAAM,